<UserControl x:Class="Frontend.Pages.MbmTabItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Frontend.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
        <TabItem VerticalAlignment="Stretch" Style="{StaticResource TabStyle}">
            <TabItem.Header>
                <StackPanel Orientation="Horizontal">
                    <Image Name="IconField" Height="25" Width="25" Source="Icons/icons8-settings-50.png"/>
                    <TextBlock Style="{StaticResource TextBlockStyle}" Text="Administration" Margin="5,0,0,0"/>
                </StackPanel>
            </TabItem.Header>
        </TabItem>
</UserControl>
